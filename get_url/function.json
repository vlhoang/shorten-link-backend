{
  "scriptFile": "app.py",
  "entryPoint": "main",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [ "get" ],
      "route": "link/{short_url}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}