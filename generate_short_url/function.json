{
  "scriptFile": "app.py",
  "entryPoint": "main",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [ "post" ],
      "route": "generate-short-url"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}